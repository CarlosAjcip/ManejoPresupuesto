@model TipoCuenta

@{
    ViewData["Title"] = "Borrar Tipo Cuenta";
}

<h3 class="mb-3">Esta seguro que desea borrar el Tipo Cuenta: @Model.Nombre?</h3>

<form asp-action="EliminarCuenta">
    <input type="hidden" asp-for="id_tiposCuen" />
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>
